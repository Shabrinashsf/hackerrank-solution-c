#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int add1(int, int);
int sub1(int, int);
float add2(float, float);
float sub2(float, float);
int main()
{
	
    int num1, num2;
    float num3, num4;
    int sum1, min1;
    float sum2, min2;
    scanf("%d %d", &num1, &num2);
    scanf("%f %f", &num3, &num4);
    sum1 = add1(num1, num2);
    sum2 = add2(num3, num4);
    min1 = sub1(num1, num2);
    min2 = sub2(num3, num4);
    
    if(num1 && num2 && num3 && num4 >= 1){
        printf("%d %d\n", sum1, min1);
        printf("%.1f %.1f", sum2, min2);
    }
    else if (num1 && num2 && num3 && num4 <= 10000){
        printf("%d %d\n", sum1, min1);
        printf("%.1f %.1f", sum2, min2);
    }
    else {
        printf("Please input the number that >= 1 and <= 10000");
    }

    return 0;
}

int add1 (int a, int b){
    return (a + b);
}
int sub1 (int a, int b){
    return (a - b);
}
float add2 (float a, float b){
    return (a + b);
}
float sub2 (float a, float b){
    return (a - b);
}
